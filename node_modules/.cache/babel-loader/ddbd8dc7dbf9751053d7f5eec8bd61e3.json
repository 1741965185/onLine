/div>\n\t\t\t\t<div class=\"transaction-right\">\n\t\t\t\t\t<div class=\"transaction-amount\">\n\t\t\t\t\t\t{{ transaction.type === 1\n\t\t\t\t\t\t\t? `${transaction.sourceAmount} ${transaction.sourceCurrency}`\n\t\t\t\t\t\t\t: `${transaction.settleAmount} ${transaction.settleCurrency}`\n\t\t\t\t\t\t}}\n\t\t\t\t\t</div>\n\t\t\t\t\t<div :class=\"['transaction-status', getStatusClass(transaction)]\">\n\t\t\t\t\t\t{{ getStatusName(transaction) }}\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\n\t\t\t<!-- Empty State -->\n\t\t\t<div v-if=\"transactions.length === 0\" class=\"empty-state\">\n\t\t\t\t<svg-icon icon-class=\"empty-icon\" class=\"empty-icon\" />\n\t\t\t\t<p>{{ $t('sysText.none') }}</p>\n\t\t\t</div>\n\t\t</div>\n\n\t\t<!-- Filter Dialog -->\n\t\t<el-dialog\n\t\t\t\t:visible.sync=\"showFilterDialog\"\n\t\t\t\t:title=\"$t('orderList.screening')\"\n\t\t\t\tcustom-class=\"filter-dialog\"\n\t\t\t\tfullscreen\n\t\t>\n\t\t\t<div class=\"filter-content\">\n\t\t\t\t<el-form :model=\"filterForm\" label-position=\"top\">\n\t\t\t\t\t<el-form-item :label=\"$t('orderList.serialOrderNumber')\">\n\t\t\t\t\t\t<el-input\n\t\t\t\t\t\t\t\tv-model=\"filterForm.TradeNo\"\n\t\t\t\t\t\t\t\t:placeholder=\"$t('orderList.serialOrderNumber')\"\n\t\t\t\t\t\t/>\n\t\t\t\t\t</el-form-item>\n\n\t\t\t\t\t<el-form-item :label=\"$t('orderList.transactionStatus')\">\n\t\t\t\t\t\t<el-select\n\t\t\t\t\t\t\t\tv-model=\"filterForm.orderstatus\"\n\t\t\t\t\t\t\t\t:placeholder=\"$t('orderList.transactionStatus')\"\n\t\t\t\t\t\t\t\tstyle=\"width: 100%\"\n\t\t\t\t\t\t>\n\t\t\t\t\t\t\t<el-option\n\t\t\t\t\t\t\t\t\tv-for=\"item in orderStates\"\n\t\t\t\t\t\t\t\t\t:key=\"item.value\"\n\t\t\t\t\t\t\t\t\t:label=\"item.label\"\n\t\t\t\t\t\t\t\t\t:value=\"item.value\"\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t</el-select>\n\t\t\t\t\t</el-form-item>\n\n\t\t\t\t\t<el-form-item :label=\"$t('orderList.transactionTime')\">\n\t\t\t\t\t\t<el-date-picker\n\t\t\t\t\t\t\t\tv-model=\"filterForm.time\"\n\t\t\t\t\t\t\t\ttype=\"dat