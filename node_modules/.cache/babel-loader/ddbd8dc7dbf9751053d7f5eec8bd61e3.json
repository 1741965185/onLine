 viewDetail = (transaction) => {\n\tif (transaction.type === 1) {\n\t\trouter.push({\n\t\t\tpath: `/details/info/${transaction.tradeNo}`,\n\t\t\tparams: { key: transaction }\n\t\t})\n\t} else {\n\t\trouter.push({ path: `/details/withdrawals/${transaction.id}` })\n\t}\n}\n\nconst resetFilter = () => {\n\tfilterForm.value = {\n\t\tTradeNo: '',\n\t\torderstatus: '',\n\t\ttime: []\n\t}\n}\n\nconst applyFilter = () => {\n\tshowFilterDialog.value = false\n\tloadTransactions()\n}\n\nconst loadTransactions = async () => {\n\t// Implementation of loading transactions based on filters\n}\n</script>\n\n<style scoped>\n.transaction-container {\n\tpadding: 16px;\n\tbackground: #f5f6fa;\n\tmin-height: 100vh;\n}\n\n.stats-section {\n\tbackground: #fff;\n\tborder-radius: 12px;\n\tpadding: 16px;\n\tmargin-bottom: 16px;\n}\n\n.stats-title {\n\tfont-size: 20px;\n\tfont-weight: 600;\n\tcolor: #364a63;\n\tmargin: 0 0 16px;\n}\n\n.stats-card {\n\tbackground: #f7f9fe;\n\tborder-radius: 8px;\n\tpadding: 12px;\n}\n\n.stats-row {\n\tdisplay: flex;\n\tjustify-content: space-between;\n\tmargin-bottom: 8px;\n}\n\n.stats-label {\n\tcolor: #8094ae;\n\tfont-size: 14px;\n}\n\n.stats-value {\n\tcolor: #364a63;\n\tfont-weight: 500;\n}\n\n.action-bar {\n\tdisplay: flex;\n\tjustify-content: space-between;\n\talign-items: center;\n\tmargin-bottom: 16px;\n}\n\n.tabs {\n\tdisplay: flex;\n\tgap: 12px;\n}\n\n.tab {\n\tpadding: 6px 12px;\n\tborder-radius: 16px;\n\tfont-size: 14px;\n\tcolor: #8094ae;\n\tbackground: #fff;\n}\n\n.tab.active {\n\tbackground: #7f8dff;\n\tcolor: #fff;\n}\n\n.action-btn {\n\twidth: 32px;\n\theight: 32px;\n\tborder-radius: 16px;\n\tbackground: #fff;\n\tborder: none;\n\tdisplay: flex;\n\talign-items: center;\n\tjustify-content: center;\n}\n\n.transaction-list {\n\tbackground: #fff;\n\tborder-radius: 12px;\n\toverflow: hidden;\n}\n\n.transaction-item {\n\tdisplay: flex;\n\tjustify-content: space-between;\n\tpadding: 16px;\n\tborder-bottom: 1px solid #ebeef4;\n}\n\n.transaction-left {\